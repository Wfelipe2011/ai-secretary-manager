    "@langchain/google-genai": "^0.0.23",
    "@langchain/langgraph": "^0.2.71",
    "@langchain/openai": "^0.5.10",
    "@langchain/redis": "^0.1.1",
    "@llamaindex/openai": "^0.1.52",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
