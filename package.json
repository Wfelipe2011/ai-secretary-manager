    "uuid": "^11.1.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
